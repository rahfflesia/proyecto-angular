generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model ejercicios {
  id                  Int    @id @default(autoincrement())
  nombre              String @db.VarChar(255)
  gifUrl              String @db.VarChar(255)
  musculosTrabajados  String @db.VarChar(255)
  parteDelCuerpo      String @db.VarChar(255)
  equipamientos       String @db.VarChar(255)
  musculosSecundarios String @db.VarChar(255)
  instrucciones       String @db.Text
}
